<div class="container mt-5">
  <div class="row">
    <div class="col-md-11">
     <table class="table table-borderless">
        <thead></thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td>
                <%= link_to user_path(user) do %>
                  <span><%= image_tag user.get_profile_image(90,90), class: 'rounded-circle mr-2' %></span>
                  <span><%= user.name %></span>
                <% end %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>