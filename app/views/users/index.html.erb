<div class="container mt-5">
  <div class="row">
    <div class="col-md-11">
      <% @users.each do |user| %>
        <div>
          <%= link_to user_path(user) do %>
            <%= user.name %>
            <%= image_tag user.get_profile_image(90,90), class: 'rounded-circle' %>
          <% end %>
        </div>
      <% end %>
    </div>
  </div>
</div>